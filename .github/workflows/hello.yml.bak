name: Hello World!

on:
  push:
    branches:
      - main

jobs:
  hello_world_job:
    runs-on: ubuntu-20.04
    steps:
      - name: Say hello
        run: |
          echo "Hello World!"
  date_job:
    runs-on: ubuntu-20.04
    steps:
      - name: Show date
        run: |
          date
  ls_job:
    runs-on: ubuntu-20.04
    steps:
      - name: List directory
        run: |
          ls
